<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Assignee alarm ui tests">
    <listeners>
        <listener class-name="org.thingsboard.server.msa.ui.listeners.RetryTestListener"/>
    </listeners>
    <test name="Assignee ui tests">
        <groups>
            <run>
                <exclude name="broken"/>
            </run>
        </groups>
        <packages>
            <package name="org.thingsboard.server.msa.ui.tests.alarmassignee.*"/>
        </packages>
    </test>
</suite>